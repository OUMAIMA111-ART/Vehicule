package casestudy1;

public class Camoin extends Vehicule{
	public int capacite;
	

	public int getCapacite() {
		return capacite;
	}


	public void setCapacite(int capacite) {
		this.capacite = capacite;
	}


	public Camoin(String marque, int capacite) {
		// TODO Auto-generated constructor stub
		super(marque);
		this.marque=marque;
		this.capacite=capacite;
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	@Override
	public double calculeprix(int nb_jour) {
		// TODO Auto-generated method stub
		this.prix=tarif_ref*(0.5)*capacite;
		return  prix;
	}
	public void affichec() {
		System.out.println("je suis une voiture de marque "+marque+".Mon capacit√© est "+capacite);
		
	}


}
